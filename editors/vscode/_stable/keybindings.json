// Place your key bindings in this file to overwrite the defaults
[
    // ===========================================================================
    // Removed
    // ===========================================================================
    {
        "key": "cmd+enter",
        "command": "-editor.action.insertLineAfter",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+enter",
        "command": "-task.new",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+cmd+enter",
        "command": "-editor.action.insertLineBefore",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+d",
        "command": "-deleteRight",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+d",
        "command": "-editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "cmd+i",
        "command": "-expandLineSelection",
        "when": "editorTextFocus"
    },
    // {
    //  "key": "shift+cmd+[",
    //  "command": "workbench.action.compareEditor.nextChange"
    // }
    // ===========================================================================
    // Native
    // ===========================================================================
    // {
    //   "key": "alt+cmd+i",
    //   "command":
    // },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.nextEditor"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "cmd+alt+c",
        "command": "workbench.action.files.copyPathOfActiveFile"
    },
    {
        "key": "ctrl+cmd+e",
        "command": "workbench.files.action.focusFilesExplorer"
    },
    {
        "key": "cmd+k cmd+i",
        "command": "workbench.action.selectIconTheme"
    },
    {
        "key": "cmd+enter",
        "command": "editor.action.insertLineAfter",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+enter",
        "command": "editor.action.insertLineBefore",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+r",
        "command": "workbench.action.reloadWindow"
    },
    {
        "key": "cmd+n",
        "command": "macros.newFileCorrectly"
    },
    {
        "key": "cmd+i",
        "command": "editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+cmd+p",
        "command": "editor.action.inspectTMScopes"
    },
    {
        "key": "shift+cmd+i",
        "command": "expandLineSelection",
        "when": "editorTextFocus"
    },
    // ===========================================================================
    // Emmet
    // ===========================================================================
    {
        "key": "shift+cmd+d",
        "command": "editor.emmet.action.balanceInward",
        "when": "editorFocus"
    },
    {
        "key": "cmd+d",
        "command": "editor.emmet.action.balanceOutward",
        "when": "editorFocus"
    },
    // ===========================================================================
    // Macros
    // ===========================================================================
    {
        "key": "ctrl+shift+b",
        "command": "macros.lineBreak"
    },
    {
        "key": "ctrl+cmd+backspace",
        "command": "macros.bsPlus"
    },
    {
        "key": "ctrl+shift+backspace",
        "command": "macros.bsPlus2"
    },
    {
        "key": "ctrl+shift+right",
        "command": "macros.downRightPaste"
    },
    {
        "key": "ctrl+shift+left",
        "command": "macros.downLeftPaste"
    },
    // ===========================================================================
    // Extensions
    // ===========================================================================
    {
        "key": "shift+alt+f",
        "command": "HookyQR.beautifyFile"
    },
    // {
    //   "key": "ctrl+shift+p",
    //   "command":"extension.vs-color-picker"
    // },
    // ===========================================================================
    // Terminal
    // ===========================================================================
    {
        "key": "ctrl+`",
        "command": "workbench.action.terminal.new"
    },
    {
        "key": "ctrl+`",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+shift+`",
        "command": "terminalTabs.createNamedTerminal"
    },
    {
        "key": "cmd+1",
        "command": "terminalTabs.showTerminal1"
    },
    {
        "key": "cmd+2",
        "command": "terminalTabs.showTerminal2"
    },
    {
        "key": "cmd+3",
        "command": "terminalTabs.showTerminal3"
    },
    {
        "key": "cmd+4",
        "command": "terminalTabs.showTerminal4"
    },
    {
        "key": "ctrl+shift+`",
        "command": "-workbench.action.terminal.new"
    },
    {
        "key": "ctrl+`",
        "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "shift+cmd+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    }
]
