{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "Icon?": true,
    "**/tags": true,
    "**/__pycache__": true,
    "**/.mypy_cache": true,
  },
  "files.associations": {
    ".{aliases,path,viminfo}": "shellscript",
    "*.blade": "blade",
    ".{babelrc,eslintrc,postcssrc,stylelintrc}": "jsonc",
    "cargo.lock": "toml",
    "Dockerfile.*": "dockerfile",
    "Cargo.lock": "toml",
    ".env": "properties",
    "*.el": "lisp",
    "*.eex": "erb",
    "*.ipynb": "json",
    "Gemfile.lock": "gemfile",
    "mix.lock": "elixir",
    "*.mak": "makefile",
    "Gopkg.lock": "toml",
    "*.{jsonc,json5,JSON-tmLanguage}": "jsonc",
    "*.{jinja,jinja2}": "html",
    ".{nvimrc,ideavimrc}": "viml",
    "*.plist": "xml",
    "*.ractive": "javascript",
    "shard.lock": "yaml",
    "*.psql": "sql",
    "*.tmpl": "gohtml",
    ".{dockerignore,eslintignore,npmignore,vscodeignore}": "gitignore",
  },
}
