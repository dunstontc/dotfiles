{
    // === Vim ===
    "vim.disableExtension": false,
    "vim.showcmd":     true,
    "vim.timeout":     2000,
    "vim.useCtrlKeys": true,
    "vim.surround":    true,
    "vim.easymotion":  false,
    "vim.incsearch":   true,
    "vim.hlsearch":    true,
    "vim.smartcase":   true,
    "vim.useSystemClipboard":    false,
    "vim.cmdLineInitialColon":   true,
    "vim.substituteGlobalFlag":  true,
    "vim.statusBarColorControl": false,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.insertModeKeyBindings": [
      {
        "before": ["<C-q>"],
        "after": ["<Esc>"]
      }
    ],
    "vim.visualModeKeyBidnings": [
      {
        "before": ["l"],
        "after": ["$"]
      },
      {
        "before": ["H"],
        "after": ["0"]
      },
    ],
    "vim.normalModeKeyBindings": [
      {
        "before": ["f", "j"], "after": [],
        "commands": [
          { "command": "workbench.action.files.save", "args": [] },
          { "command": "workbench.action.closeActiveEditor", "args": [] }
        ]
      },
      {
        "before": [","],
        "after": ["i", "<space>", "<esc>"]
      },
      {
        "before": ["<bs>"],
        "after": ["a", "<bs>", "<esc>", "<right>"]
      },
      // {
      //   "before": ["L"],
      //   "after": ["$"]
      // },
      // {
      //   "before": ["H"],
      //   "after": ["0"]
      // },
      {
        "before": ["U"],
        "after": ["<C-r>"]
      },
      {
        "before": ["<leader>", "<leader>"],
        "after": ["<Esc>"],
      },
      {
        "before": ["<leader>", "l"], "after": [],
        "commands": [{ "command": "editor.action.format", "args": [] }]
      },
      {
        "before": ["<leader>", "f", "t"],
        "after": [],
        "commands": [{ "command": "workbench.action.toggleSidebarVisibility", "args": [] }]
      },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys":{
      // "<C-a>": false,
      // "<C-q>": false,
      "<C-f>": false,
      "<C-e>": false,
      "x": false,
      "/": false,
      // "<C-q>": true,
    },
}
