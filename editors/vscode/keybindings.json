[
  // ===========================================================================
  // CMD+K
  // ===========================================================================
  {
    "key": "cmd+k cmd+i",  // Icons
    "command": "workbench.action.selectIconTheme"
  },
  {                        // Extensions
    "key": "cmd+k cmd+e",
    "command": "workbench.extensions.action.listOutdatedExtensions"
  },
  {
    "key": "cmd+k cmd+l",  // Language
    "command": "workbench.action.editor.changeLanguageMode",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+m",  // Mappings
    "command": "workbench.action.openGlobalKeybindingsFile"
  },
  {
    "key": "cmd+k cmd+o",  // Open file from git diff view
    "command": "git.openFile"
  },
  {
    "key": "cmd+k cmd+p",  // Pretty handy...
    "command": "editor.action.inspectTMScopes"
  },
  {                        // Recent
    "key": "cmd+k cmd+r",
    "command": "workbench.action.openRecent"
  },
  {                        // View
    "key": "cmd+k cmd+v",
    "command": "markdown.showPreviewToSide"
  },
  // ===========================================================================
  // Native
  // ===========================================================================
  {
    "key": "ctrl+tab",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "ctrl+shift+tab",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "cmd+alt+c",
    "command": "workbench.action.files.copyPathOfActiveFile"
  },
  {
    "key": "ctrl+cmd+e",
    "command": "workbench.files.action.focusFilesExplorer"
  },
  {
    "key": "cmd+enter",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+enter",
    "command": "editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.reloadWindow"
  },
  {
    "key": "cmd+i",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+cmd+p",
    "command": "editor.action.inspectTMScopes"
  },
  {
    "key": "shift+cmd+i",
    "command": "expandLineSelection",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.files.save"
  },
  {
    "key": "ctrl+d",
    "command": "editor.action.copyLinesUpAction",
  },
  // ===========================================================================
  // Emmet
  // ===========================================================================
{
    "key": "shift+alt+f",
    "command": "HookyQR.beautifyFile"
  },
  // {
  //   "key": "ctrl+shift+p",
  //   "command":"extension.vs-color-picker"
  // },
  // ===========================================================================
  // Terminal
  // ===========================================================================
  {
    "key": "ctrl+`",
    "command": "workbench.action.terminal.new"
  },
  {
    "key": "ctrl+`",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+shift+`",
    "command": "terminalTabs.createNamedTerminal"
  },
  // ===========================================================================
  // Removed
  // ===========================================================================
  {
    "key": "cmd+enter",
    "command": "-editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+enter",
    "command": "-task.new",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+cmd+enter",
    "command": "-editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+d",
    "command": "-deleteRight",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+i",
    "command": "-expandLineSelection",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenView"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenNavigateNextInViewPicker"
  },
  {
    "key": "cmd+k cmd+m",
    "command": "-workbench.extensions.action.showRecommendedKeymapExtensions"
  },
  {
    "key": "shift+cmd+c",
    "command": "-workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  {
    "key": "cmd+k p",
    "command": "-workbench.action.files.copyPathOfActiveFile"
  },
]
